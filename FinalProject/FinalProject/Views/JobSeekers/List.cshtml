@model IEnumerable<FinalProject.Models.JobSeeker>

@{
    ViewBag.Title = "List";
}

<h2>Jobseekers    <img style='height:50px; width:90px'  src="@Url.Content("http://www.d3mconsulting.com/uploads/6/2/7/0/6270712/301312_orig.jpg")"></h2>

@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.State)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Info)
        </th>
        <th>
            Aptitude Score
        </th>

        <th>
            Personality Type
        </th>

        <th>
            Emotional Intelligence
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>

        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.City)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.State)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Info)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Survey1Score)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Survey2Score)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Survey3Score)
        </td>

        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
